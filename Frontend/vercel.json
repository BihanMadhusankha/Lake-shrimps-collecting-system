{
    "version": 2,
    "builds": [
      {
        "src": "src/index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "source": "/feedback/:id",
        "destination": "/api/feedback/:id"
      },
      {
        "source": "/(.*)",
        "destination": "/src/index.html"
      }
    ]
  }